{
  "name": "wdiodemoapp",
  "version": "1.0.8",
  "private": true,
  "scripts": {
    "android.clean": "cd android && gradlew.bat clean && cd ..",
    "android.dev": "npm run android.clean && npm run build.version.android && react-native run-android",
    "android.release": "cd android && gradlew.bat clean && gradlew.bat assembleRelease",
    "android.release.git": "cd android && ./gradlew clean && ./gradlew assembleRelease",
    "android.release.ci": "cd android && ./gradlew clean && ./gradlew assembleRelease --no-daemon",
    "test.mobile": "npx wdio run wdio.conf.js",
    "ios.dev": "npm run ios.pods && npm run build.version.ios && react-native run-ios",
    "ios.pods": "cd ios && pod install && cd ..",
    "ios.release.sim.build": "npm run ios.pods && react-native run-ios --mode=Release",
    "lint": "eslint .",
    "start": "react-native start",
    "start.ci": "react-native start --reset-cache",
    "test": "jest",
    "test.ci": "npx wdio run wdio.conf.js --spec tests/specs/test.spec.js --no-watch",
    "build.version.android": "react-native-version -A -b --target android",
    "build.version.ios": "react-native-version -A -b -L --target ios",
    "build.versions": "react-native-version -A -L",
    "postinstall": "patch-package",
    "allure:generate": "allure generate allure-results --clean",
    "allure:open": "allure open allure-report"
  },
  "dependencies": {
    "@react-native-picker/picker": "^2.6.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/material-bottom-tabs": "^6.2.19",
    "@react-navigation/native": "^6.1.9",
    "expo": ">=50.0.0-0 <51.0.0",
    "expo-local-authentication": "^13.6.0",
    "react": "18.2.0",
    "react-native": "0.73.1",
    "react-native-biometrics": "^3.0.1",
    "react-native-bootsplash": "^5.2.0",
    "react-native-confetti-cannon": "^1.5.2",
    "react-native-device-info": "^10.12.0",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-paper": "^5.11.4",
    "react-native-picker-select": "^9.0.0",
    "react-native-reanimated": "^3.6.1",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-webview": "^13.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.73.18",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@react-native/typescript-config": "^0.73.1",
    "@types/babel__core": "^7.20.5",
    "@types/babel__preset-env": "^7.9.6",
    "@types/eslint": "^8.56.0",
    "@types/prettier": "3.0.0",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "@wdio/allure-reporter": "^9.5.0",
    "@wdio/appium-service": "^9.5.7",
    "@wdio/cli": "^9.5.7",
    "@wdio/local-runner": "^9.5.7",
    "@wdio/mocha-framework": "^9.5.0",
    "@wdio/spec-reporter": "^9.5.0",
    "@wdio/visual-service": "^6.3.0",
    "appium": "^2.15.0",
    "appium-uiautomator2-driver": "^3.9.9",
    "babel-jest": "^29.6.3",
    "chai": "^5.1.2",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "mocha": "^11.0.1",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "2.8.8",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4",
    "wdio-wait-for": "^3.0.11",
    "webdriverio": "^9.5.7"
  },
  "engines": {
    "node": ">=18"
  }
}